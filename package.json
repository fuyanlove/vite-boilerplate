{
    "name": "vue3",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "@vueuse/head": "^0.7.5",
        "axios": "^0.25.0",
        "element-plus": "^1.3.0-beta.10",
        "pinia": "^2.0.11",
        "vue": "^3.2.25",
        "vue-router": "^4.0.12",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@types/node": "^17.0.16",
        "@vitejs/plugin-vue": "^2.0.0",
        "csslab": "^4.0.3",
        "husky": "^7.0.4",
        "less": "^4.1.2",
        "lint-staged": "^12.3.5",
        "typescript": "^4.4.4",
        "vite": "^2.7.2",
        "vue-tsc": "^0.29.8"
    },
    "lint-staged": {
        "*.{js,vue}": [
            "prettier-eslint --write",
            "eslint --fix --max-warnings=0 --no-ignore",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run test:unit"
        }
    }
}
